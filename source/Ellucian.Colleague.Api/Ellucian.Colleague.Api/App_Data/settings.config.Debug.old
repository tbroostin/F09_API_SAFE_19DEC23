<!--
The following transform is setup to only replace specific values (i.e. the 
xdt:Transform attribute is on each element that should be updated).
-->
<settings xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <colleague>
    <accountName xdt:Transform="Replace">testEnv_rt</accountName>
    <ipAddress xdt:Transform="Replace">testdmihost</ipAddress>
    <port xdt:Transform="Replace">0</port>
    <sharedSecret xdt:Transform="Replace">encryptedsharedsecretfortest</sharedSecret>
    <dasEnvironment xdt:Transform="Replace">testEnv</dasEnvironment>
    <dasAddress xdt:Transform="Replace">testDAShost</dasAddress>
    <dasPort xdt:Transform="Replace">0</dasPort>
    <dasLogin xdt:Transform="Replace">encryptedDASloginfortest</dasLogin>
    <dasPassword xdt:Transform="Replace">encryptedDASpasswordfortest</dasPassword>
  </colleague>
</settings>